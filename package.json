{
    "name": "moment-ex-tool",
    "version": "1.0.4",
    "description": "moment's extended tool library does not change moment's original logic, it just subtly calls moment.",
    "scripts": {
        "test": "ts-node ./scripts/jest.ts",
        "build": "ts-node ./scripts/build.ts",
        "clear:node_modules": "rimraf node_modules"
    },
    "dependencies": {
        "moment": "^2.29.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^21.0.3",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.1",
        "@types/jest": "^27.4.1",
        "@types/lodash": "^4.14.181",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "eslint": "^8.12.0",
        "jest": "^27.5.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3"
    },
    "types": "./dist/@types/index.d.ts",
    "unpkg": "./dist/umd/moment-ex-tool.min.js",
    "jsdelivr": "./dist/iife/moment-ex-tool.min.js",
    "browser": "./dist/umd/moment-ex-tool.ex.min.js",
    "main": "./dist/umd/moment-ex-tool.ex.min.js",
    "module": "./dist/esm/moment-ex-tool.min.js",
    "exports": {
        ".": {
            "import": "./dist/esm/moment-ex-tool.min.js",
            "require": "./dist/umd/moment-ex-tool.ex.min.js"
        }
    },
    "repository": "https://github.com/fidelyiu/moment-ex-tool.git",
    "author": "Fidel Yiu <fidel-yiu@foxmail.com>",
    "license": "MIT"
}
